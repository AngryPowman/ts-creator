{
  "name": "ts-creator",
  "version": "1.1.5",
  "description": "A code generator to generate TypeScript code generator from TypeScript code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "test": "tsc -p ./tsconfig.test.json && node ./dist/tests/index.js",
    "lint": "prettier --config ./prettier.json --list-different ./src/**/* ./tests/**/*",
    "prettier": "prettier --config ./prettier.json --write ./src/**/* ./tests/**/*"
  },
  "author": "kingwl",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.15.2",
    "prettier": "^1.15.3",
    "rollup": "^1.1.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "typescript": "^3.2.2"
  },
  "files": [
    "src",
    "dist",
    "LICENSE",
    "tsconfig.json",
    "package.json",
    "README.md"
  ]
}
