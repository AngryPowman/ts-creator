{
  "name": "ts-creator",
  "version": "1.1.1",
  "description": "A code generator to generate TypeScript code generator from TypeScript code",
  "main": "dist/index.js",
  "types": "dist/types",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "test": "tsc -p ./tsconfig.test.json && node ./dist/tests/index.js",
    "lint": "prettier --config ./prettier.json --list-different ./src/**/* ./tests/**/*",
    "prettier": "prettier --config ./prettier.json --write ./src/**/* ./tests/**/*"
  },
  "author": "kingwl",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.12.18",
    "@types/prettier": "^1.15.2",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  },
  "files": [
    "src",
    "prettier.json",
    "dist"
  ]
}
